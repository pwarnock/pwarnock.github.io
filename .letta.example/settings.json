{
  "localSharedBlockIds": {
    "project": "block-<UUID-from-letta-cli>"
  },
  "pathPlaceholder": "{{PROJECT_ROOT}}",
  "permissions": {
    "allow": [
      "Bash(cd {{PROJECT_ROOT}} && git status:*)",
      "Bash(npm audit:*)",
      "Bash(pwd:*)",
      "Bash(npm run dev:status:*)",
      "Bash(npm run version:*)"
    ]
  }
}
